{
  "Version": "1.0.0",
  "Name": "Ystervark",
  "Description": "Asp.NET Core 2 Web API + AngularJS Front-end",
  "AppSettings": {
    "EnableLogging": true,
    "LoginUrl": "/login",
    "RedirectUri": "http://localhost:46664/login"
  },
  "Api": {
    "Host": "http://localhost",
    "EndPoint": "/Ystervark.API/api"
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  }
}
